{
  "name": "bitcore-electrum",
  "version": "0.0.4",
  "description": "A bitcore addon to allow for electrum style key derivation.",
  "main": "Electrum.js",
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/coyno/bitcore-electrum.git"
  },
  "keywords": [
    "bitcoin",
    "electrum",
    "bitcore"
  ],
  "author": "Coyno UG (haftungsbeschr√§nkt)",
  "bugs": {
    "url": "https://github.com/coyno/bitcore-electrum/issues"
  },
  "homepage": "https://github.com/coyno/bitcore-electrum",
  "scripts": {
    "start": "node build/start.js",
    "babelstart": "babel-node lib/start.js",
    "preview": "babel-watch --watch lib lib/start.js",
    "build": "babel lib --out-dir build --copy-files --source-maps --ignore test.js",
    "prebuild": "rm -rf build",
    "test": "mocha -t 5000 -u tdd --compilers js:babel-core/register \"./{,!(node_modules)/**/}*.test.js\"",
    "dev": "mocha --watch -t 5000 -u tdd --compilers js:babel-core/register \"./{,!(node_modules)/**/}*.test.js\"",
    "lint": "standard"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-watch": "^2.0.6",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-subset": "^1.4.0",
    "eslint": "^3.16.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "flow-bin": "^0.40.0",
    "mocha": "^3.2.0",
    "randomstring": "^1.1.5",
    "standard": "^8.6.0",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "bitcore-lib": "^0.14.0"
  }
}
